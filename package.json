{
  "name": "supportops-admin",
  "private": true,
  "packageManager": "pnpm@9.0.0",
  "scripts": {
    "dev": "pnpm -r --parallel dev",
    "dev:web": "pnpm --filter @supportops/web dev",
    "dev:theme": "pnpm --filter @supportops/ui-theme dev",
    "build": "tsc -b && pnpm -r --sort build",
    "build:web": "pnpm --filter @supportops/web build",
    "typecheck": "tsc -b --noEmit",
    "typecheck:watch": "tsc -b -w --noEmit",
    "lint": "pnpm -r lint",
    "lint:fix": "pnpm -r lint --fix",
    "test": "pnpm -r test",
    "test:watch": "pnpm -r test --watch",
    "clean": "pnpm -r exec rm -rf dist .turbo .next",
    "clean:all": "pnpm clean && rm -rf node_modules && pnpm -r exec rm -rf node_modules",
    "format": "prettier --write \"**/*.{ts,tsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,json,md}\"",
    "graph": "pnpm list --depth=Infinity --graph-type=dependencies",
    "deps:graph": "pnpm list --depth=Infinity --json > deps-graph.json"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.7",
    "@mui/material": "^7.3.7",
    "@remixicon/react": "^4.6.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@pnpm/list": "^1000.2.5",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "tsup": "^8.5.1",
    "typescript": "^5",
    "typescript-eslint": "^8.54.0",
    "yaml": "^2.4.2"
  }
}
