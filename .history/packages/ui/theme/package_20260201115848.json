{
  "name": "@supportops/ui-theme",
  "private": true,
  "version": "0.0.0",
  "sideEffects": false,
  "scripts": {
    "dev": "tsup src/index.ts --watch --format esm,cjs --dts",
    "build": "tsup src/index.ts --format esm,cjs --dts --out-dir dist --clean",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "lint": "eslint ."
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "peerDependencies": {
    "react": "^19",
    "react-dom": "^19",
    "@mui/material": "^7",
    "@emotion/react": "^11",
    "@emotion/styled": "^11"
  }
}
