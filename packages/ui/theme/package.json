{
  "name": "@supportops/ui-theme",
  "private": true,
  "version": "0.0.0",
  "type": "module", 
  "sideEffects": false,
  
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    },
    "./components": {
      "types": "./dist/components/index.d.ts",
      "import": "./dist/components/index.js",
      "require": "./dist/components/index.cjs"
    },
    "./package.json": "./package.json"
  },
  
  "files": [
    "dist",
    "README.md"
  ],
  
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "typecheck": "tsc --noEmit",
    "lint": "eslint ."
  },
  
  "peerDependencies": {
    "react": "^19",
    "react-dom": "^19",
    "@mui/material": "^7",
    "@emotion/react": "^11",
    "@emotion/styled": "^11"
  },
  
  "devDependencies": {
    "@types/react": "^19",
    "tsup": "^8",
    "typescript": "^5"
  }
}